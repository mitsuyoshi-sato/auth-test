{
  "name": "db",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "migrate:deploy": "pnpm exec prisma migrate deploy",
    "migrate:dev": "pnpm exec prisma migrate dev",
    "seed": "pnpm exec tsx prisma/seed.ts",
    "lint": "pnpm exec biome check ./src",
    "fix": "pnpm exec npm-run-all -p fix:*",
    "fix:prisma": "pnpm exec prisma format",
    "fix:biome": "pnpm exec biome check --write ./src",
    "generate": "pnpm exec prisma generate",
    "studio": "pnpm exec prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.19.23",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
